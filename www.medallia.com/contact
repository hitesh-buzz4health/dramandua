<script>
  ;(function() {
    function getURLParameter (name) {
    	  return decodeURIComponent((new RegExp('[?|&]' + name + '=' + '([^&;]+?)(&|#|;|$)').exec(location.search)||[null,""])[1].replace(/\+/g, '%20'))||null;
    }


    function capitalize(word) {
      return word[0].toUpperCase() + word.substr(1);
    }

    /**
     * Replaces underscores with spaces and capitalizes each word.
     * e.g. 'e-book:_the_retailers_guide_to_competing_with_amazon'
     * becomes 'E-Book: The Retailers Guide To Competing With Amazon'
     */
    function titlfy(title) {
      return title
        .split('-').map(capitalize).join('-')
        .split('_').map(capitalize).join(' ');
    }

    jQuery(function($) {
      var utm_h1 = getURLParameter('utm_h1');

      if (utm_h1) {
        var newTitle = titlfy(utm_h1);

        jQuery('#utm_h1').text(newTitle);
      }
      $('#utm_h1').addClass('active');
    })
  })();

  jQuery(document).ready(function($) {
    $('body').on('click', '.back-to-top', function() {
      $('html, body').animate({scrollTop: 0}, 'slow');
    });
  });
</script>
